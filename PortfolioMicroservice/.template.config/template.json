{
  "$schema": "http://json.schemastore.org/template",
  "author": "Leo506",
  "classifications": [
    "Web",
    "API",
    "Backend",
    "Microservice"
  ],
  "name": "PortfolioMicroService",
  "description": "This template was created for easy and fast development small services without authorization and authentication",
  "identity": "PortfolioMicroService",
  "sourceName": "PortfolioMicroService",
  "shortName": "PMS",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0"
        }
      ],
      "defaultValue": "net6.0"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            ".vs/**",
            ".idea/**",
            "_ReSharper.Caches/**",
            ".template_config/**",
            ".git/**",
            ".git*"
          ]
        }
      ]
    }
  ]
}