{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "mongo" : "mongodb://mongodb:27017"
  },
  "Products" : {
    "Collection" : "ProductCollection",
    "Database" : "ProductDb"
  },
  "Kafka" : {
    "IsEnable" : true,
    "ProducerConfig" : {
      "Topic" : "Product_Created",
      "KafkaHost" : "broker:9092",
      "AcksSetting" : "Leader"
    }
  }
}
